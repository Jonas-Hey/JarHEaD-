= Snapshots in History =
 
The following snapshots are currently available:
<br/>
 
{{#widget: ActionableResult | 
 columnConfiguration = {{ 
      {{ variableName = 'date'
       | valueResolver = 'DATETIME'
      }} }}
 | rowActions = {{ 
      {{ confirm = 'Are you sure you want to delete this snapshot and all associated data?'
       | args = {{ '?:snapshotId' }}
       | method = 'deleteSnapshot'
       | label = 'Delete'
       | useJob = true
       | onFinish = 'reload'
       | clazz = 'com.fluidops.iwb.history.HistoricalDataManagement'
       | render = 'img:/ajax/icons/delete.png'
      }} }}
 | query = '
SELECT ?snapshotId ?snapshotType ?provider ?date
WHERE {
  GRAPH history:MetaContext {
    ?provider rdf:type System:Provider .
    ?snapshotId rdf:type history:Snapshot .
    ?provider ?hasSnapshot ?snapshotId .
    ?snapshotId historySnapshot:type ?snapshotType .
    ?snapshotId dc:date ?date .
  }
}
ORDER BY ASC(?date)'
 | repository = 'history'
 | asynch = true
}}
