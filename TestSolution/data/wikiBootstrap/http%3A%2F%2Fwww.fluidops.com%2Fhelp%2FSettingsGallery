    | width = '400'
   }}
}}
</td>
</tr>
<tr>
<td>
====== Stacked Bars ======
Settings:[[Settings:barvertical.stacked.example]]
{{#widget: BarChart | 
 input = 'Org'
 | variableConfiguration = {{ 
      {{ variableName = 'projectsA'
       | displayName = 'national projects'
      }} | 
      {{ variableName = 'projectsB'
       | displayName = 'international projects'
      }} }}
 | query = '
    SELECT ?Org ?projectsA ?projectsB WHERE { 
        VALUES (?Org ?projectsA ?projectsB )
        { 
          ("Org 1" 23 3)
          ("Org 2" 56 45)
          ("Org 3" 33 12)
          ("Org 4" 87 65)
          ("Org 5" 20 8)
          ("Org 6" 15 10)
          ("Org 7" 90 32)
          ("Org 8" 55 40)
          ("Org 9" 6 3)
   } }'
 | settings = 'Settings:barvertical.stacked.example'
 | output = {{ 'projectsA' | 'projectsB' }}
 | styling = 
   {{ height = '200'
    | width = '400'
   }}
}}
</td>
<td>

====== Stacked Bars 100% ======
