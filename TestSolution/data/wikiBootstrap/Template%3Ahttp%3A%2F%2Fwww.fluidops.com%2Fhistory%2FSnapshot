 | asynch = true
}}
|}

== Actions ==

{{#widget: CodeExecution | 
 confirm = 'Are you sure you want to delete this snapshot and all associated data?'
 | args = {{ $this$ }}
 | method = 'deleteSnapshot'
 | label = 'Delete'
 | useJob = true
 | onFinish = 'history:History'
 | clazz = 'com.fluidops.iwb.history.HistoricalDataManagement'
}}

