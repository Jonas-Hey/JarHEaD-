	WHERE { VALUES (?label ?tendency ?percent ?value) { 
	("active" "up" 2.4 "89T") ("active" "neutral" 0.4 "7T") 
	("active" "up" 2.4 "83T") ("inactive" "down" 0.4 "12T")} }
}}

</source>
