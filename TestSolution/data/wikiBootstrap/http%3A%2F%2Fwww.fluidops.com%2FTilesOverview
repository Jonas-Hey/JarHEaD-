| query7=SELECT ("Physical Hosts" as ?label) ("56" as ?value) WHERE {}
| query8=SELECT ("VM" as ?label) ("23" as ?value) WHERE {}
| query9=SELECT ("Memory" as ?label) (13194139533312 as ?value) WHERE {}
| query9valueResolver=BYTE_HUMAN_FRIENDLY
| query10=SELECT ("Memory Used" as ?label) ("40,33 %" as ?value) ("yellow" as ?color) WHERE {}
