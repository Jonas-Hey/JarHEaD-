===== Value Pairs Multiple Queries ===== 

{{Template:fluidops:ValuePairsMultiQueryTile | heading=Compute Infrastructure | headingLink=fluidops:ComputeInfrastructure 
| query1=SELECT ("Physical Hosts" as ?label) ("34" as ?value) WHERE {}
| query2=SELECT ("VM" as ?label) ("5" as ?value) WHERE {}
| query3=SELECT ("Memory" as ?label) (22539988369408 as ?value) WHERE {}
| query3valueResolver=BYTE_HUMAN_FRIENDLY
| query4=SELECT ("Memory Used" as ?label) ("72,33 %" as ?value) ("red" as ?color) WHERE {}
| query5=SELECT ("CPU Cores" as ?label) ("26" as ?value) WHERE {}
| query6=SELECT ("CPU Used" as ?label) ("50, %" as ?value) ("yellow" as ?color) WHERE {}
}}
</div>
<div style="width:40%;height:450px;float:right;font-size:12px;margin-left:50px;">

<div style="margin-top:80px;height:50px;"> Source Code </div><source>

{{Template:fluidops:ValuePairsMultiQueryTile | heading=Compute Infrastructure | headingLink=fluidops:ComputeInfrastructure 
| query1=SELECT ("Physical Hosts" as ?label) ("34" as ?value) WHERE {}
| query2=SELECT ("VM" as ?label) ("5" as ?value) WHERE {}
| query3=SELECT ("Memory" as ?label) (22539988369408 as ?value) WHERE {}
| query3valueResolver=BYTE_HUMAN_FRIENDLY
| query4=SELECT ("Memory Used" as ?label) ("72,33 %" as ?value) ("red" as ?color) WHERE {}
| query5=SELECT ("CPU Cores" as ?label) ("26" as ?value) WHERE {}
| query6=SELECT ("CPU Used" as ?label) ("50, %" as ?value) ("yellow" as ?color) WHERE {}
}}

</source>
</div>
