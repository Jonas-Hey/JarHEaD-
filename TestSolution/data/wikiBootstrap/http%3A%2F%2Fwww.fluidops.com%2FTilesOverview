===== Value Pairs ===== 

{{Template:fluidops:ValuePairsTile | heading=Compute Infrastructure | headingLink=fluidops:ComputeInfrastructure 
| query1=SELECT ?label ?value ?color WHERE {
   # TODO: Missing the constraint for infrastrucutre that "belongs" to the user
   VALUES (?label ?value ?color)
   { ("Physical Hosts" "34" UNDEF)
     ("VM" "5" UNDEF)
     ("Memory" "20 TB" UNDEF)
     ("Memory Used" "72,33 %" "red")
     ("CPU Cores" "26" UNDEF)
     ("CPU Used" "50,5 %" "yellow")
   }
}
}}
</div>
<div style="width:40%;height:450px;float:right;font-size:12px;margin-left:50px;">

<div style="margin-top:80px;height:50px;"> Source Code </div><source>

{{Template:fluidops:ValuePairsTile 
| heading=Compute Infrastructure 
| headingLink=fluidops:ComputeInfrastructure 
| query1=SELECT ?label ?value ?color WHERE {
   VALUES (?label ?value ?color)
   { ("Physical Hosts" "34" UNDEF)
     ("VM" "5" UNDEF)
     ("Memory" "20 TB" UNDEF)
     ("Memory Used" "72,33 %" "red")
     ("CPU Cores" "26" UNDEF)
     ("CPU Used" "50,5 %" "yellow")
   }
}
