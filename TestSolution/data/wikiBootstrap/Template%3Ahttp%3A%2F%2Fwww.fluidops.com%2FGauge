<!-- Requiered parameter: percent number (1 - 100) to show -->

{{#widget: BarChart | 
 input = 'used'
 | query = 'select ?used ?free
            where { BIND( "used" as ?used) BIND(100 as ?free)}'
 | settings = 'Settings:gauge'
 | output = {{ 'free' }}
 | styling = 
   {{ height = '160'
    | width = '160'
   }}
 | customMappings = 'value={{{percent|0}}}'
}}



