== Actions ==
<div style="float:left;">
{{#widget: CodeExecution | 
 confirm = 'Are you sure you want to delete the context including all its triples?'
 | args = {{ $this$ }}
 | method = 'deleteContext'
 | passContext = true
 | label = 'Delete'
 | onFinish = 'Admin:ContentOverview'
 | clazz = 'com.fluidops.iwb.api.Context'
}}
</div>
<div style="float:left;">
{{#widget: CodeExecution | 
 args = {{ $this$ | '%format' | '%exportMeta' }}
 | method = 'exportContext'
 | passContext = true
 | userInput = {{ 
      {{ componentType = 'DROPDOWN'
       | displayName = 'Export Format (RDF/XML is recommended for Protege)'
       | name = 'format'
       | presetValue = '"RDF/XML"'
       | selectValues = {{ '"RDF/XML"' | '"Turtle"' | '"N3"' | '"TriG"' | '"N-Triples"' }}
       | required = false
      }} | 
      {{ componentType = 'CHECKBOX'
       | displayName = 'include contexts meta information'
       | name = 'exportMeta'
       | presetValue = 'checked'
       | required = false
      }} }}
 | label = 'Export'
 | onFinish = 'none'
 | clazz = 'com.fluidops.iwb.api.Context'
}}
</div>
<div style="clear: left">&nbsp;</div>