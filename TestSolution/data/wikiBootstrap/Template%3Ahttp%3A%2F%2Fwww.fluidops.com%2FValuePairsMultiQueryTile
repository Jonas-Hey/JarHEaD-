<!-- 
The template can visualize up to 6 values (label + value) in a tile. The template expects a query, a heading and a heading link as parameters. The query results contains labels, values and colors (the font color for value pairs).

	the parameter {{{heading}}} - heading of the tile (optional)
	the parameter {{{headingLink}}} - the link behind the heading (optional)
	the parameter {{{colspan}}} - the tile size: 1 or 2. Default is 1 (optional)
	the parameter {{{query1}}} to {{{query6}}} - the query returning 'label' and 'value' result set to be displayed in the chart. The query result is displayed in the corresponding segment of the first column (at least one required)
	the parameter {{{query7}}} to {{{query12}}} - the query returning 'label' and 'value' result set to be displayed in the chart. The query result is displayed in the corresponding segment of the second column (optional)
	the parameter {{{query1valueResolver}}} to {{{query12valueResolver}}} - value resolver for the value of the corresponding query
-->

<div data-ss-colspan="{{{colspan|1}}}" class="keyvaluetile tile{{{colspan|1}}}x1">
<div class="tilecontent">
{{Template:fluidops:TileHeading  | heading={{{heading|}}}| headingLink={{{headingLink|}}} | description={{{description|}}} }}
<div class="tiledata">

{{#if: {{{query1|}}} |
{{Template:ValuePairsTileSegment | query={{{query1|SELECT ?label ?value WHERE{}}}} | valueResolver={{{query1valueResolver|DEFAULT}}} }}
}}
{{#if: {{{query2|}}} |
{{Template:ValuePairsTileSegment | query={{{query2|SELECT ?label ?value WHERE{}}}} | valueResolver={{{query2valueResolver|DEFAULT}}} }}
}}
{{#if: {{{query3|}}} |
{{Template:ValuePairsTileSegment | query={{{query3|SELECT ?label ?value WHERE{}}}} | valueResolver={{{query3valueResolver|DEFAULT}}} }}
}}
{{#if: {{{query4|}}} |
{{Template:ValuePairsTileSegment | query={{{query4|SELECT ?label ?value WHERE{}}}} | valueResolver={{{query4valueResolver|DEFAULT}}} }}
}}
{{#if: {{{query5|}}} |
{{Template:ValuePairsTileSegment | query={{{query5|SELECT ?label ?value WHERE{}}}} | valueResolver={{{query5valueResolver|DEFAULT}}} }}
}}
{{#if: {{{query6|}}} |
{{Template:ValuePairsTileSegment | query={{{query6|SELECT ?label ?value WHERE{}}}} | valueResolver={{{query6valueResolver|DEFAULT}}} }}
}}

</div>

{{#if: {{{query7|}}} |
<div class="tiledata">
{{Template:ValuePairsTileSegment | query={{{query7|SELECT ?label ?value WHERE{}}}} | valueResolver={{{query7valueResolver|DEFAULT}}} }}

{{#if: {{{query8|}}} |
{{Template:ValuePairsTileSegment | query={{{query8|SELECT ?label ?value WHERE{}}}} | valueResolver={{{query8valueResolver|DEFAULT}}} }}
}}
{{#if: {{{query9|}}} |
{{Template:ValuePairsTileSegment | query={{{query9|SELECT ?label ?value WHERE{}}}} | valueResolver={{{query9valueResolver|DEFAULT}}} }}
}}
{{#if: {{{query10|}}} |
{{Template:ValuePairsTileSegment | query={{{query10|SELECT ?label ?value WHERE{}}}} | valueResolver={{{query10valueResolver|DEFAULT}}} }}
}}
{{#if: {{{query11|}}} |
{{Template:ValuePairsTileSegment | query={{{query11|SELECT ?label ?value WHERE{}}}} | valueResolver={{{query11valueResolver|DEFAULT}}} }}
}}
{{#if: {{{query12|}}} |
{{Template:ValuePairsTileSegment | query={{{query12|SELECT ?label ?value WHERE{}}}} | valueResolver={{{query12valueResolver|DEFAULT}}} }}
}}
</div>
}}



<div style="clear:left;"></div>

</div>
</div>